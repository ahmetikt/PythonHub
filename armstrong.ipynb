   "source": [
    "deger = input('give a number: ')\n",
    "liste = list(deger)\n",
    "total = 0\n",
    "\n",
    "for i in liste:\n",
    "    basamak = (int(i)) ** len(liste)\n",
    "    total += basamak\n",
    "if int(deger) == total:\n",
    "        print(f'{total} is an Armstrong number')\n",
    "else:\n",
    "        print(f'{deger} is not an Armstrong number')"
   ]
